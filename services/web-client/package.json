{
    "name": "web-client",
    "scripts": {
        "dev": "run-p validate:dev sapper:dev",
        "prod": "run-s validate sapper:build",
        "start": "node __sapper__/build",
        "validate": "svelte-check --ignore src/node_modules/@sapper",
        "validate:dev": "svelte-check --ignore src/node_modules/@sapper --watch",
        "sapper:dev": "sapper dev -p 6701",
        "sapper:build": "cross-env NODE_ENV=production sapper build -p 6701"
    },
    "devDependencies": {
        "@tsconfig/svelte": "1.0.10",
        "@types/compression": "1.7.0",
        "@types/node": "14.14.2",
        "@types/polka": "0.5.1",
        "autoprefixer": "10.0.1",
        "cross-env": "7.0.2",
        "css-loader": "5.0.0",
        "cssnano": "4.1.10",
        "dotenv": "8.2.0",
        "esbuild-loader": "2.4.0",
        "file-loader": "6.1.1",
        "npm-run-all": "4.1.5",
        "postcss": "8.1.2",
        "postcss-loader": "4.0.4",
        "sapper": "0.28.10",
        "sass": "1.27.0",
        "sass-loader": "10.0.3",
        "style-loader": "2.0.0",
        "svelte": "3.29.0",
        "svelte-check": "1.0.55",
        "svelte-loader-hot": "0.3.1",
        "svelte-materialify": "0.1.13",
        "svelte-preprocess": "4.5.1",
        "tslib": "2.0.3",
        "typescript": "4.0.3",
        "webpack": "4.44.2"
    },
    "dependencies": {
        "@mdi/font": "5.7.55",
        "compression": "1.7.4",
        "polka": "0.5.2",
        "sirv": "1.0.6"
    }
}
